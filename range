def gerar_tabuada(numero):
    print(f"Tabuada de {numero}:")
    for i in range(1, 11):  # Loop de 1 a 10
        resultado = numero * i
        print(f"{numero} X {i} = {resultado}")

while True:
    num = int(input("Digite um número para ver a tabuada (de 1 a 10): "))
    if 1 <= num <= 10:
        gerar_tabuada(num)
    else:
        print("Número fora da faixa permitida (de 1 a 10).")
